{
  "manifest_version": 3,
  "name": "Scrappr for Google Docs",
  "version": "1.0",
  "description": "Smart note-taking and suggestions for Google Docs.",
  "permissions": [
    "storage",
    "scripting",
    "activeTab",
    "clipboardRead" 
  ],
  "action": {
    "default_popup": "notes.html"
  },
  "icons": {
    "16": "assets/scrappr.png",
    "48": "assets/scrappr.png",
    "128": "assets/scrappr.png"
  }
}